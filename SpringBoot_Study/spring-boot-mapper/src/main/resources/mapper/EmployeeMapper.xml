<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ling.mapper.EmployeeMapper">

    <select id="selectByPrimaryKey" resultType="com.ling.domain.Employee">
        select *
        from employee
        where id = #{id}
    </select>

    <select id="selectList" resultType="com.ling.domain.Employee">
        select *
        from employee
    </select>
</mapper>